<p id="notice"><%= notice %></p>

<h1>Pushes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>When</th>
      <th>User</th>
      <th>Doctor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pushes.each do |push| %>
      <tr>
        <td><%= push.title %></td>
        <td><%= push.when %></td>
        <td><%= push.user_id %></td>
        <td><%= push.doctor_id %></td>
        <td><%= link_to 'Show', push %></td>
        <td><%= link_to 'Edit', edit_push_path(push) %></td>
        <td><%= link_to 'Destroy', push, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Push', new_push_path %>
