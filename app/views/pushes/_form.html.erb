<%= form_for(push) do |f| %>
  <% if push.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(push.errors.count, "error") %> prohibited this push from being saved:</h2>

      <ul>
      <% push.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :when %>
    <%= f.datetime_select :when %>
  </div>

  <div class="field">
    <%= f.label :user_id %>
    <%= f.number_field :user_id %>
  </div>

  <div class="field">
    <%= f.label :doctor_id %>
    <%= f.text_field :doctor_id %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
